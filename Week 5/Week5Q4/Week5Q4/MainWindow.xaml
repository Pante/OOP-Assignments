<Window x:Class="Week5Q4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Week5Q4"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="sda" Content="Enter a number between 1 and 100 (or -1 to end):" HorizontalAlignment="Left" Margin="33,62,0,0" VerticalAlignment="Top" Width="272"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="26" Margin="310,62,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130" TextChanged="textBox_TextChanged"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Height="27" Margin="88,118,0,0" VerticalAlignment="Top" Width="310"/>

    </Grid>
</Window>
